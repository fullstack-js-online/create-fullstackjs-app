{
  "name": "<%= name %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node server/index.js",<% if (database === 'mysql') { %>
    "migrate": "knex migrate:latest",
