    "redux": "^4.0.1",
    "redux-axios-middleware": "^4.0.0",
    "redux-flash": "^2.0.1",<% } %><% if (ui === 'tailwind') { %>
    "tailwindcss": "^0.7.4",<% } %><% if (client === 'vue') { %>
    "vee-validate": "^2.2.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vuex": "^3.1.0",<% } %>
    "yup": "^0.27.0"
