    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/preset-env": "^7.3.4",<% if (client === 'react') { %>
    "@babel/preset-react": "^7.0.0",<% } %><% if (client === 'vue' && test === 'jest') { %>
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",<% } %>
    "babel-eslint": "^10.0.1",<% if (test === 'jest') { %>
    "babel-jest": "^24.5.0",<% } %>
